{
  "name": "ollama-fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel build:watch copy:watch",
    "build": "tsc && vite build",
    "build:watch": "vite build --watch",
    "copy:watch": "nodemon -e js,ts,json -w dist/ -x \"ncp dist/ ../public/\"",
    "preview": "vite preview"
  },
  "devDependencies": {
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.6.2",
    "vite": "^6.0.1"
  },
  "dependencies": {
    "nodemon": "^3.1.7"
  }
}
